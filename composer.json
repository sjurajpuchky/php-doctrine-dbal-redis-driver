{
    "name": "baba/php-doctrine-dbal-redis-driver",
    "description": "Simple implementation of Doctrine DBAL Driver for redis",
    "type": "library",
    "require": {
        "doctrine/dbal": "*",
        "doctrine/orm": "*",
        "php": ">=7.3.0",
        "ext-redis": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Doctrine\\DBAL\\Driver\\Redis\\": "src/Driver/",
            "Doctrine\\ORM\\": "src/ORM/",
            "Doctrine\\DBAL\\Platforms\\": "src/Platforms/"
        }
    },
    "provide": {
        "baba/php-doctrine-dbal-redis-driver": "1.0.0.0"
    },
